          <b:widget-setting name='content'><![CDATA[<script type="text/javascript">
  function generateSitemap() {
    const blogUrl = "https://feiticeiravip.com.br"; // Substitua pelo URL do seu Blogger
    const xmlHeader = '<?xml version="1.0" encoding="UTF-8"?>\n<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">\n';
    const xmlFooter = '</urlset>';
    let xmlBody = "";

    // Busca os feeds de postagens do Blogger
    fetch(blogUrl + "/feeds/posts/default?alt=json&max-results=500")
      .then(response => response.json())
      .then(data => {
        const posts = data.feed.entry || [];
        posts.forEach(post => {
          const postUrl = post.link.find(link => link.rel === "alternate").href;
          const postDate = post.published.$t.split("T")[0]; // Formato: YYYY-MM-DD

          // Criação das tags XML para cada postagem
          xmlBody += `
  <url>
    <loc>${postUrl}</loc>
    <lastmod>${postDate}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.8</priority>
  </url>`;
        });

        // Junta o sitemap completo
        const sitemap = xmlHeader + xmlBody + xmlFooter;

        // Exibe no gadget e permite copiar
        const sitemapContainer = document.getElementById("sitemapXml");
        sitemapContainer.textContent = sitemap;
      })
      .catch(error => {
        console.error("Erro ao gerar o sitemap:", error);
      });
  }

  // Gera o sitemap automaticamente ao carregar a página
  window.onload = generateSitemap;
</script>

<div>
